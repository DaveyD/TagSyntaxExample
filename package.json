{
    "name": "tagxp",
    "displayName": "tag-xp",
    "description": "An extension for TagXP syntax highlighting.",
    "icon": "icon/TAG Icon 2.png",
    "version": "0.0.1",
    "publisher": "DMD",
	"repository": {
		"type": "git",
		"url": "https://github.com/DaveyD/TagSyntaxExample.git"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages", "Themes"
    ],
    "contributes": {
        "languages": [{
            "id": "tagxp",
            "aliases": ["Tag", "tag"],
            "extensions": [".prg"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "tagxp",
            "scopeName": "source.tagxp",
            "path": "./syntaxes/log.tmLanguage.json"
        }],
        "themes": [{
            "label": "Dark+TagXP",
            "language": "tagxp",
            "uiTheme": "vs-dark",
            "path": "./themes/tagxp-theme.json"
        }]
    }
}